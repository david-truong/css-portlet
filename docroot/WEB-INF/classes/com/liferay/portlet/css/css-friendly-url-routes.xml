<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">

<routes>
	<route>
		<pattern>/global.css</pattern>
		<generated-parameter name="scope">global</generated-parameter>
		<implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
		<implicit-parameter name="p_p_state">exclusive</implicit-parameter>
	</route>
	<route>
		<pattern>/private/groupId/{groupId:\d+}/site.css</pattern>
		<generated-parameter name="privateLayout">true</generated-parameter>
		<generated-parameter name="scope">site</generated-parameter>
		<implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
		<implicit-parameter name="p_p_state">exclusive</implicit-parameter>
	</route>
	<route>
		<pattern>/public/groupId/{groupId:\d+}/site.css</pattern>
		<generated-parameter name="privateLayout">false</generated-parameter>
		<generated-parameter name="scope">site</generated-parameter>
		<implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
		<implicit-parameter name="p_p_state">exclusive</implicit-parameter>
	</route>
	<route>
		<pattern>/plid/{plid:\d+}/page.css</pattern>
		<generated-parameter name="scope">page</generated-parameter>
		<implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
		<implicit-parameter name="p_p_state">exclusive</implicit-parameter>
	</route>
</routes>